apigenDir=${config[apigenDir]};

apigenCf='pub_doc.neon';
echo $apigenCf;

read -p 'change apigen dir? ' cad
if [ $cad ]; then
    read -p '$apigenDir' apigenDir;
fi

read -p 'change apigen config file? ' cacf
if [ $cacf ]; then
    read -p '$apigenCf' apigenCf;
fi


apigen --config  $apigenDir/$apigenCf;





